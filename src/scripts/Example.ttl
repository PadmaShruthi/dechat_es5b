@prefix : <#> .
@prefix msg: <http://schema.org/> .
@prefix user: <https://user.solid.community/> .
@prefix sender: <https://sender.solid.community/> . 

:timeStamp
    a msg:message ;
    msg:date "#La fecha" ;
    msg:from sender ;
    msg:to user ;
    msg:text "#El Mensaje" .
    

